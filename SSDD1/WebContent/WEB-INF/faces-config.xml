<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
	    <from-view-id>/pages/index1.xhtml</from-view-id>
	    <navigation-case>
	        <from-outcome>ID</from-outcome>
	        <to-view-id>/pages/BusquedaByID.xhtml</to-view-id>
	        <redirect></redirect>
	    </navigation-case>
	    <navigation-case>
	        <from-outcome>DISTRITO</from-outcome>
	        <to-view-id>/pages/BusquedaByDistrito.xhtml</to-view-id>
	        <redirect></redirect>
	    </navigation-case>
	    <navigation-case>
	        <from-outcome>UGEL</from-outcome>
	        <to-view-id>/pages/BusquedaByUGEL.xhtml</to-view-id>
	        <redirect></redirect>
	    </navigation-case>	    	    	    
	</navigation-rule>
</faces-config>
